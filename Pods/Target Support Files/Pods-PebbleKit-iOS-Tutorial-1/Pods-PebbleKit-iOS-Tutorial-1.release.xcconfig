FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/PebbleKit"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/PebbleKit"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/PebbleKit"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "CoreGraphics" -framework "Foundation" -framework "PebbleKit" -framework "UIKit" -weak_framework "CoreBluetooth" -weak_framework "ExternalAccessory"
PODS_ROOT = ${SRCROOT}/Pods
